.goods-desc {
  padding: 0rpx 38rpx;
}
.goods-desc > .start , .end{
  width: 180rpx;
  height: 2rpx;
  background-color: #333;
  position: relative;
}

.end {
  float: right;
}

.start::before{
  content: "";
  width: 10rpx;
  height: 10rpx;
  background-color: #333;
  position: absolute;
}

.end::after{
  content: "";
  width: 10rpx;
  height: 10rpx;
  background-color: #333;
  position: absolute;
  bottom: 0;
  right: 0;
}

.goods-desc .desc {
  color: #666;
  font-size: 32rpx;
  margin: 20rpx 0;
}

.goods-images {
  border-bottom: 12rpx solid #eee;
  margin-top: 24rpx;
}

.goods-images .item-image {
  width: 100%;
}

.goods-images .images-desc {
  color: #666;
  font-size: 36rpx;
  margin: 32rpx 32rpx;
}